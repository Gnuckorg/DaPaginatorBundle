{
    "name": "da/paginator-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2's helper bundle for pagination",
    "keywords": ["symfony", "helper", "pagination", "paginator", "bundle"],
    "homepage": "http://github.com/Gnuckorg/DaPaginatorBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Prelot",
            "email": "tprelot@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/dependency-injection" : ">=2.1",
        "symfony/config" : ">=2.1",
        "pagerfanta/pagerfanta": "dev-master",
        "twig/twig": "*",
        "symfony/twig-bundle": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Da\\PaginatorBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Da/PaginatorBundle"
}